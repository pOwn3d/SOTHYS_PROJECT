<section class="section">
    <div class="container">
        <div class="row">
            <div class="col-12 aos-init aos-animate" data-aos="fade-up">
                {% for name, gamme in prices %}
                    <h2>
                        {{ name }}
                    </h2>

                    {% for genericName, products in gamme %}
                        <h3>
                            {{ genericName }}
                        </h3>

                        <table>
                            <thead>
                                <td>
                                    {{ 'Product' | trans }}
                                </td>
                                <td>
                                    {{ 'Code' | trans }}
                                </td>
                                <td>
                                    {{ 'Code barre' | trans }}
                                </td>
                                <td>
                                    {{ 'price.unit' | trans }}
                                </td>
                                <td>
                                    {{ 'quantity' | trans }}
                                </td>
                            </thead>
                            {% for price in products %}
                            <tr>
                                <td>
                                    {{ price.label(app.request.locale) }}
                                </td>
                                <td>
                                    {{ price.itemId }}
                                </td>
                                <td>
                                    {{ price.codeEAN }}
                                </td>
                                <td>
                                    {{ price.itemPrices.first().price|number_format(2) }}
                                </td>
                                <td>
                                    {{ price.capacity(app.request.locale) }}
                                </td>
                            </tr>
                            {% endfor %}
                        </table>
                    {% endfor %}
                {% endfor %}
            </div>
        </div>
    </div>
<section>
